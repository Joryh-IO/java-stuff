public class Main {

     static int happynumber(int n){
        int sum = 0;
        while(n!=0){
            int digit = n%10;
            sum += digit*digit;
            n = n/10;
        }
        System.out.println(sum);
        if(sum ==1 || sum== 4)return sum;
        else happynumber(sum);
        return sum;
    }


    static String rem(String s, char a){
         String ans="";
         for(int i=0; i<s.length(); i++){
             if(s.charAt(i)!=a)ans += s.charAt(i);
         }
         return ans;
    }

